<%= simple_form_for([@organization, @custom_domain], class: "contents") do |form| %>
  <div class="grid grid-cols-6 gap-6">
    <div class="col-span-12 sm:col-span-5 lg:col-span-3">
      <%= form.input :url, label: 'URL', placeholder: "e.g. https://qr.yourdomain.com" %>
    </div>
  </div>
  <div class="mt-3">
    <%= render_component 'button', type: :submit do %>
      Submit
    <% end %>
  </div>
<% end %>
